{
    "name": "nagivateto",
    "displayName": "Grouped Symbol Search",
    "description": "Allows you to quickly find classes, methods, functions or variables.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.43.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:nagivateto.rebuildIndex",
        "onCommand:nagivateto.navigateToClass",
        "onCommand:nagivateto.navigateToMethod",
        "onCommand:nagivateto.navigateToFunction",
        "onCommand:nagivateto.navigateToVariable"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "nagivateto.rebuildIndex",
                "title": "NavigateTo: Rebuild index"
            },
            {
                "command": "nagivateto.navigateToClass",
                "title": "NavigateTo: Class"
            },
            {
                "command": "nagivateto.navigateToMethod",
                "title": "NavigateTo: Method"
            },
            {
                "command": "nagivateto.navigateToFunction",
                "title": "NavigateTo: Function"
            },
            {
                "command": "nagivateto.navigateToVariable",
                "title": "NavigateTo: Variable"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "lint": "eslint src --ext ts",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn run compile && yarn run lint",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^7.0.2",
        "@types/node": "^13.11.0",
        "@types/vscode": "^1.43.0",
        "@typescript-eslint/eslint-plugin": "^2.26.0",
        "@typescript-eslint/parser": "^2.26.0",
        "electron": "7.1.11",
        "electron-rebuild": "^1.10.1",
        "eslint": "^6.8.0",
        "glob": "^7.1.6",
        "mocha": "^7.1.1",
        "typescript": "^3.8.3",
        "vscode-test": "^1.3.0"
    },
    "dependencies": {
        "fuse.js": "^5.1.0",
        "tree-sitter": "^0.16.1",
        "tree-sitter-javascript": "^0.16.0",
        "tree-sitter-python": "^0.16.1"
    }
}
